        div#navbar
            div#logo 
                img(src="/static/img/logo.png")
                | HDA
            ul
                li #[a(href="/") Home]
                li #[a(href="/") About Us]
                li #[a(href="/") Services]
                li #[a(href="/") Class Info]
                li #[a(href="/") Contact Us]
        div#introsection
