                div.card2.card
                    h3 Dance the way you like
                    div.card-box 
                        div.card-img 
                            img(src="/static/img/2.png")
                        div.card-content 
                            p This is the best dance academy of the universe 
                               
                div.card3.card
                    h3 Expert Teaching
                    div.card-box 
                        div.card-img 
                            img(src="/static/img/3.png")
